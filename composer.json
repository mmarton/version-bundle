  "description": "Mautic Version bundle",
  "type": "mautic-plugin",
  "version": "0.5",
  "require": {
    "php": "^7.4|^8.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-strict-rules": "^1.0",
    "phpunit/phpunit": "^9.5",
